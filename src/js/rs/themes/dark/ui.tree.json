{
    "DefViews": {
        "color" : "%{zebkit.ui.palette.gray}",
        "font"  : "12px 'Lucida Grande', 'Lucida Sans', Arial, sans-serif"
    },

    "BaseTree" : {
        "lineColor" : "red",
        "color"     : "red",
        "itemGapX"  : 4,
        "itemGapY"  : 4,
        "views": {
            "off"     : { "$zebkit.ui.ToggleView": [ true, "white", "red", 14 ], "br" : { "$zebkit.ui.RoundBorder": [ "orange" ] }  },
            "on"      : { "$zebkit.ui.ToggleView": [ false, "lightGray", "red", 14 ], "br" : { "$zebkit.ui.RoundBorder": [ "orange" ] }  },
            "aselect" : { "$zebkit.ui.Border" : [ "orange", 2, 6 ] },
            "iselect" : { "$zebkit.ui.Border" : [ "gray", 2, 6 ] }
        }
    },

    "DefViews" : {
        "color" : "red",
        "font"  : "bold"
    },

    "Tree": {
        "inheritProperties" : true
    },

    "CompTree": {
        "Checkbox" : {
            "inheritProperties" : true,
            "Label"   : {
                "color"   : "red",
                "font"    : "bold"
            }
        },

        "Combo" : {
            "inheritProperties" : true,
            "background" : null,
            "border"     : "%{zebkit.ui.borders.plain}",
            "CompList" : {
                "Label" : {
                    "font" : "bold",
                    "color" : "red"
                }
            },
            "List" : {
                "ViewProvider" : {
                    "font" : "bold",
                    "color": "red"
                }
            }
        },

        "Label" : {
            "inheritProperties" : true,
            "color"   : "red",
            "font"    : "bold",
            "padding" : 4
        },

        "inheritProperties" : true,

        "views": {
            "close"   : null,
            "open"    : null,
            "leaf"    : null,

            "off"     : { "$zebkit.ui.ToggleView": [ false, "white", "red", 14  ] },
            "on"      : { "$zebkit.ui.ToggleView": [ "true", "white", "red", 14 ] },
            "iselect" : { "$zebkit.ui.Border" : [ "lightGray", 1, 6 ]  },
            "aselect" : { "$zebkit.ui.Border" : [ "white", 1, 6 ]  }
        }
    }
}